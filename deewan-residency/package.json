{
  "name": "deewan-residency",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:check": "tsc -b && vite build && npm run preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:network": "vite preview --host 0.0.0.0",
    "ip": "node -e \"const os = require('os'); const nets = os.networkInterfaces(); Object.keys(nets).forEach(name => { nets[name].forEach(net => { if (net.family === 'IPv4' && !net.internal) console.log('Your IP:', net.address); }); });\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@gsap/react": "^2.1.2",
    "@sanity/client": "^7.12.1",
    "@sanity/image-url": "^1.2.0",
    "@tabler/icons-react": "^3.35.0",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "gsap": "^3.13.0",
    "lightswind": "^3.1.18",
    "lucide-react": "^0.552.0",
    "motion": "^12.23.24",
    "next-sanity": "^11.6.6",
    "react": "^19.2.0",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^7.9.5",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
